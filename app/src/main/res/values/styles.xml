<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="common_divider_line">
        <item name="android:background">@color/common_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>
    <style name="order_payment_lable_item" parent="@style/order_payment_lable">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/order_payment_layout_bg</item>
        <item name="android:paddingLeft">@dimen/common_container_content_gap</item>
        <item name="android:paddingRight">@dimen/common_container_content_gap</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">47.0dip</item>
    </style>
    <style name="order_payment_lable">
        <item name="android:textSize">@dimen/t4</item>
        <item name="android:textColor">@color/c2</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/common_content_bg</item>
        <item name="android:paddingLeft">@dimen/common_container_content_gap</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">42.0dip</item>
    </style>
    <style name="tv_round_corner_red_stroke_bg">
        <item name="android:textSize">@dimen/t9</item>
        <item name="android:textColor">@color/common_red</item>
        <item name="android:background">@drawable/round_corner_red_stroke_bg</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:paddingBottom">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="util_bottombar_navigatebutton">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/sl_main_button</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="SeekBar" parent="@android:style/Widget">
        <item name="android:thumb">@drawable/bg_play_pause</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/bg_seekbar</item>
        <item name="android:indeterminateDrawable">@drawable/bg_seekbar</item>
        <item name="android:minHeight">50dip</item>
        <item name="android:maxHeight">50dip</item>
        <item name="android:thumbOffset">0dip</item>
        <item name="android:focusable">true</item>
    </style>

    <style name = "HYTheme.Theme" parent="@android:style/Theme.Holo.Light.NoActionBar.TranslucentDecor" tools:targetApi="kitkat">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>



    <style name="TransparentTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!--item name="homeAsUpIndicator">@drawable/abc_ic_ab_back_holo_dark</item-->
        <item name="android:actionBarStyle">@style/ActionBarStyle.Transparent</item>
        <item name="actionBarStyle">@style/ActionBarStyle.Transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="windowActionBarOverlay">true</item>
    </style>

    <style name="ActionBarStyle.Transparent" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:background">@null</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="android:titleTextStyle">@style/ActionBarStyle.Transparent.TitleTextStyle</item>
        <item name="titleTextStyle">@style/ActionBarStyle.Transparent.TitleTextStyle</item>
    </style>

    <style name="ActionBarStyle.Transparent.TitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="TTNotificationTitle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>


    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
    </style>

    <style name="AppCompatTheme" parent="AppBaseTheme">

        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>

        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

        <!-- Toolbar Theme / Apply white arrow -->
        <item name="colorControlNormal">@android:color/white</item>

    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>

    <style name="TTNotificationText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="ui_parent_Wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="city_hot_header" parent="@style/ui_parent_Wrap">
        <item name="android:textSize">@dimen/city_list_text_size</item>
        <item name="android:textColor">@color/city_item_letter_color</item>
        <item name="android:duplicateParentState">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="cityLocatingProgressStyle">
        <item name="android:maxWidth">16.0dip</item>
        <item name="android:maxHeight">24.0dip</item>
        <item name="android:indeterminateDrawable">@anim/city_locating_anim</item>
        <item name="android:minWidth">16.0dip</item>
        <item name="android:minHeight">24.0dip</item>
    </style>

    <style name="lovehanyang_actionsheet" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:padding">0dp</item>
        <!--<item name="android:listPreferredItemPaddingStart">0dp</item>-->
        <!--<item name="android:listPreferredItemPaddingEnd">0dp</item>-->
    </style>
    <style name="subscribe_seperate_line">
        <item name="android:background">#ffcdcdcd</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginTop">14.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>

    <style name="chat_text_date_style">
        <item name="android:padding">2dp</item>
        <item name="android:paddingTop">2dip</item>
        <item name="android:paddingBottom">2dip</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
        <item name="android:textColor">@color/btn_gray_normal</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/timestampe_bg</item>
    </style>
    <style name="chat_content_date_style">
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="chat_text_name_style">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="devide_line_eee">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.5dp</item>
        <item name="android:background">#eee</item>
    </style>
    <style name="MyDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

</resources>
